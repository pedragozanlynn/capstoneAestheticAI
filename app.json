{
  "expo": {
    "name": "AestheticAI",
    "slug": "frontend",
    "owner": "nllynnnn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons.png",
    "scheme": "aestheticai",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icons.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#FFFFFF",
      "bundleIdentifier": "com.pedragozanlynn.aestheticai"
    },
    "android": {
      "package": "com.pedragozanlynn.aestheticai",
      "icon": "./assets/icons.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "resize",

      "permissions": [
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VIDEO",
        "READ_MEDIA_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons.png",
      "backgroundColor": "#FFFFFF"
    },
    "plugins": [
      "expo-router",
      "expo-media-library"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "description": "AestheticAI Expo app",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f36d0693-2179-4f28-bbe3-2ec975543a18"
      }
    }
  }
}
